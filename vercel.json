{
    "framework": "vite",
    "buildCommand": "cd client && pnpm install && pnpm run build",
    "outputDirectory": "client/dist",
    "builds": [
        {
            "src": "server/app.py",
            "use": "@vercel/python"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server/app.py"
        }
    ]
}
